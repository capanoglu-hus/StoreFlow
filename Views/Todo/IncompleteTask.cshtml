@model List<string>
@{
    ViewData["Title"] = "IncompleteTask";
    Layout = "~/Views/Layout/Index.cshtml";
    int count = 0 ;
}

<div class="row">
    <div class="col-lg-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h3 class="card-title"> Kontrol Todo listesi(append prepend) </h3>
                <p class="card-description">
                    <code>kategori işlemlerinizi buradan yapabilirsiniz</code>
                </p>
                <div class="table-responsive">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>#</th>
                                <th>Açıklama</th>
                                
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                count++;
                                <tr>
                                    <th>@count</th>
                                    <td>@item</td>
                                   
                                   
                                </tr>
                            }

                        </tbody>
                    </table>
                   
                </div>
            </div>
        </div>
    </div>
</div>


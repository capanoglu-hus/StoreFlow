<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0"> Kategori Sayısı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.categoryCount</h2>
                
                                </div>
                                <small class="text-gray">toplamda @ViewBag.categoryCount kategori bulunur </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-chart-pie text-info icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0">En yüksek ürün fiyatı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.productMaxprice TL</h2>
                                </div>
                                <small class="text-gray"> Bu ürün : @ViewBag.productMaxPriceProductName</small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-shopping-cart text-danger icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0">En Düşük Ürün Fiyatı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.productMinprice TL</h2>
                                    
                                </div>
                                <small class="text-gray">Bu ürün : @ViewBag.productMinPriceProductName</small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-bell text-info icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0"> Toplam Ürün Stok Sayısı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.totalSumProductStock</h2>

                                </div>
                                <small class="text-gray">Stokta  @ViewBag.totalSumProductStock ürün bulunur </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-comment  text-warning icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0">Ortalama Ürün Stok Sayısı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.AvgProductStock </h2>
                                </div>
                                <small class="text-gray"> Toplam Ürün Sayısı / Ürün Adet Değeri </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-cubes text-primary icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0">Ortalama Ürün Fiyatı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.AvgProductPrice TL</h2>

                                </div>
                                <small class="text-gray"> Bir adet ürün içn ortalam fiyat</small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-desktop text-secondary icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0"> 1000 TL yüksek ürün sayısı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.biggerPriceThen1000Product</h2>

                                </div>
                                <small class="text-gray">Fiyatı 1000 TL yüksek ürün sayısı </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-clock  text-success icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0">4 nolu Id ürün</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h3 class="mb-0">@ViewBag.getIdis4ProductName </h3>
                                </div>
                                <small class="text-gray"> 4 nolu Id ürün adı </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-coffee text-dark icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0"> Stok sayısı 50-100 arası sayısı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.StockCountBigger50andSmaller100ProductCount</h2>

                                </div>
                                <small class="text-gray"> Stok sayısı 50-100 arası sayısını getirir </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-filter text-info icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0"> 1000 TL yüksek ürün sayısı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.biggerPriceThen1000Product</h2>

                                </div>
                                <small class="text-gray">Fiyatı 1000 TL yüksek ürün sayısı </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-clock  text-success icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0">4 nolu Id ürün</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h3 class="mb-0">@ViewBag.getIdis4ProductName </h3>
                                </div>
                                <small class="text-gray"> 4 nolu Id ürün adı </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-coffee text-dark icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-4 grid-margin">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title mb-0"> Stok sayısı 50-100 arası sayısı</h4>
                        <div class="d-flex justify-content-between align-items-center">
                            <div class="d-inline-block pt-3">
                                <div class="d-md-flex">
                                    <h2 class="mb-0">@ViewBag.StockCountBigger50andSmaller100ProductCount</h2>

                                </div>
                                <small class="text-gray"> Stok sayısı 50-100 arası sayısını getirir </small>
                            </div>
                            <div class="d-inline-block">
                                <i class="fas fa-filter text-info icon-lg"></i>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
       

    </div>
    <!-- content-wrapper ends -->
    <!-- partial:../partials/_footer.html -->
    <footer class="footer">
        <div class="d-sm-flex justify-content-center justify-content-sm-between">
            <span class="text-muted text-center text-sm-left d-block d-sm-inline-block">Copyright © 2018 <a href="https://www.urbanui.com/" target="_blank">Urbanui</a>. All rights reserved.</span>
            <span class="float-none float-sm-right d-block mt-1 mt-sm-0 text-center">Hand-crafted & made with <i class="far fa-heart text-danger"></i></span>
        </div>
    </footer>
    <!-- partial -->
</div>